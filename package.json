{
  "name": "websocket-app",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build:server": "nest build -p ./server/tsconfig.server.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "run-p start:server start:builded:client",
    "start:builded:client": "run-p build:client start:client",
    "start:client": "webpack serve --config client/webpack.dev.js",
    "build:client": "webpack --config client/webpack.dev.js",
    "test:client": "react-scripts test",
    "eject:client": "react-scripts eject",
    "nest:start": "nest start",
    "start:server": "run-p build:server nest:start",
    "debug:server": "nest start --debug --watch",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typecheck:client": "tsc --noEmit -p client/tsconfig.client.json"
  },
  "dependencies": {
    "@nestjs/cli": "^8.2.2",
    "@nestjs/common": "^8.4.0",
    "@nestjs/core": "^8.4.0",
    "@nestjs/platform-express": "^8.4.0",
    "@nestjs/platform-socket.io": "^8.4.0",
    "@nestjs/schematics": "^8.0.7",
    "@nestjs/testing": "^8.4.0",
    "@nestjs/websockets": "^8.4.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.26",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.12",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "css-loader": "^6.6.0",
    "file-loader": "^6.2.0",
    "log4js": "^6.4.2",
    "mini-css-extract-plugin": "^2.5.3",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^5.0.0",
    "rimraf": "^3.0.2",
    "socket.io": "^4.4.1",
    "socket.io-client": "^4.4.1",
    "styled-components": "^5.3.3",
    "ts-loader": "^9.2.7",
    "typescript": "^4.6.2",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "7.16.0",
    "@babel/plugin-proposal-optional-chaining": "7.14.5",
    "@babel/plugin-proposal-private-property-in-object": "7.16.7",
    "@babel/plugin-transform-runtime": "7.14.5",
    "@babel/preset-react": "7.14.5",
    "@babel/preset-typescript": "7.15.0",
    "@babel/runtime": "7.14.6",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jest": "27.4.1",
    "@types/lodash": "^4.14.179",
    "@types/node": "^14.14.36",
    "@types/styled-components": "^5.1.18",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "clean-webpack-plugin": "4.0.0",
    "dotenv-webpack": "7.0.3",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.2.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.10.1",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.3.5",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^3.11.3",
    "webpack-merge": "5.8.0",
    "webpack-node-externals": "3.0.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
